	top: 143px;
	left: 31px;
	text-indent: -9999px;
}
.slider-box .btn-prev:hover,
.slider-box .btn-next:hover{opacity: 0.8;}
.slider-box .btn-next{
	background-position: -42px -77px;
	right: 31px;
