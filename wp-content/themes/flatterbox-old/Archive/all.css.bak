}
.carousel {
	position: relative;
}
.carousel .mask {
	position: relative;
	overflow: hidden;
	width: 600px;
	margin: 0 auto;
	min-height: 330px;
	padding: 28px 0 32px;
}
.carousel .mask .slideset {
