#status-bar{
	list-style: none;
	margin: 0 0 34px;
	padding: 0;
	overflow: hidden;
	background: #fff;
}
#status-bar li{
	font: 19px/22px 'Sanchez', Arial, Helvetica, sans-serif;
	float: left;
	text-align: center;
	position: relative;
}
#status-bar li.done{
	position: relative;
}
#status-bar li.done:after,
#status-bar li a:after{
	content: '';
	background: url(images/sprite.png) no-repeat 0 -127px;
	width: 46px;
	height: 60px;
	position: absolute;
	right:-44px;
	top: 2px;
	z-index: 9;
}
#status-bar li.done:after,
#status-bar li.done a:after,
#status-bar li.active a:after{
	background: url(images/sprite.png) no-repeat -141px -127px;
	width: 44px;
	height: 65px;
	top: 0px;
}
#status-bar li.active{
	position: relative;
	z-index: 1;
}
#status-bar li.done{
	position: relative;
	z-index: 9;
}
#status-bar li.active a:after{
	background: url(images/sprite.png) no-repeat -71px -127px;
	height: 61px;
	top: 2px;
}
#status-bar li.active a span{text-decoration: underline;}
#status-bar li.last a:after{background: none;}
#status-bar li a{
	display: block;
	position: relative;
	z-index: 1;
	color: #000;
}
#status-bar li a span{
	padding: 17px 0 21px 45px;
	display: block;
	border-style: solid;
	border-color: #082850;
	border-width: 2px 0;
}
#status-bar li.last a span{border-right-width: 2px;}
#status-bar li.done a span,
#status-bar li.active a span{
	background: #082850;
	color: #fff;
}
#status-bar li.done a span{ border-left: 2px solid #fff; }
#status-bar li.done a span {
	border-color: #fff;
}
#status-bar li:first-child a span{padding-left: 0;}
#status-bar li.last a span{padding-right: 30px;}
